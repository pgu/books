<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .title {
            font-weight: bold;
            width: 300px;
            font-size: 2.5em;
            padding: 2px 20px 10px 5px;
        }
        .suggestBox {
            width: 300px;
            margin: 10px;
        }
    </ui:style>
    <g:HorizontalPanel>
        <g:Label ui:field="title" addStyleNames="{style.title}">Libros Harriet</g:Label>
        <g:SuggestBox ui:field="suggestBox" addStyleNames="{style.suggestBox}"></g:SuggestBox>
        <g:Button ui:field="busca">Busca</g:Button>
    </g:HorizontalPanel>
</ui:UiBinder> 