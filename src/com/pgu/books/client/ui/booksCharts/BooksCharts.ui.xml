<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:HTMLPanel>
        <g:HorizontalPanel>
            <g:VerticalPanel>
                <g:RadioButton name="chartType" ui:field="btnArea" >Area Chart</g:RadioButton>
                <g:RadioButton name="chartType" ui:field="btnBar" >Bar Chart</g:RadioButton>
                <g:RadioButton name="chartType" ui:field="btnColumn" >Column Chart</g:RadioButton>
                <g:RadioButton name="chartType" ui:field="btnLine" >Line Chart</g:RadioButton>
                <g:RadioButton name="chartType" ui:field="btnPie" checked="true">Pie Chart</g:RadioButton>
            </g:VerticalPanel>
            <g:HTMLPanel ui:field="charts">
            </g:HTMLPanel>
        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder> 