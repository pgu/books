<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .title {
            font-size: 2.5em;
            padding: 2px 2px 10px 5px;
        }
    </ui:style>
    <g:HorizontalPanel ui:field="menu" >
        <g:Hyperlink ui:field="booksLink"  addStyleNames="{style.title}"/>
        <g:Hyperlink ui:field="graphsLink" addStyleNames="{style.title}"/>
        <g:Hyperlink ui:field="importLink" addStyleNames="{style.title}"/>
    </g:HorizontalPanel>
</ui:UiBinder> 